#include <bits/stdc++.h>
using namespace std;

int main()
{
    //Max heap
    priority_queue<int> pq;
    pq.push(100);
    pq.push(20);
    pq.push(50);
    pq.push(10);
    pq.push(60);
    cout<<"size: "<<pq.size()<<endl;
    cout<<"top: "<<pq.top()<<endl;
    while(!pq.empty()){
        cout<<pq.top()<<" ";
        pq.pop();
    }
    cout<<endl;
    //min heap
    priority_queue<int,vector<int>,greater<int> > p;
    p.push(100);
    p.push(20);
    p.push(50);
    p.push(10);
    p.push(60);
    while(!p.empty()){
        cout<<p.top()<<" ";
        p.pop();
    }
    return 0;
}
