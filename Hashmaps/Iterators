#include <bits/stdc++.h>
using namespace std;

int main()
{
    unordered_map<string ,int> m;
    m["abc"]=1;
    m["abc2"]=2;
    m["abc3"]=3;
    m["abc4"]=4;
    m["abc5"]=5;
    unordered_map<string ,int>::iterator it=m.begin();
    while(it != m.end()){
        cout<<"key: "<<it->first<<" value: "<<it->second<<endl;
        it++;
    }
    unordered_map<string ,int>:: iterator it1=m.find("abc2");
    while(it1 != m.end()){
        cout<<"key: "<<it1->first<<" value: "<<it1->second<<endl;
        it1++;
    }
    // for vectors
    vector<int> v;
    v.push_back(10);
    v.push_back(20);
    v.push_back(30);
    v.push_back(40);
    vector<int>::iterator it2=v.begin();
    while(it2 != v.end()){
        cout<<*it2<<endl;
        it2++;
    }
    return 0;
}
